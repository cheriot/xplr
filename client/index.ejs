<!doctype html>
<html lang='en'>
  <head>
    <meta charset='utf-8'>
    <meta http-equiv='X-UA-Compatible' content='IE=edge'>
    <meta name='viewport' content='width=device-width, initial-scale=1'>
    <meta name='theme-color' content='#CFBC9E'>
    <link rel='stylesheet' href="[[*-bundle-*.css]]" />
    <title>Xplr</title>
  </head>
  <body>
    <section><%- reactHtml %></section>
    <script type='text/javascript'>
      var mapInitialized = false;
      var mapInitializers = [];
      function initMap() {
        mapInitializers.forEach(function(initializer) {
          initializer();
        });
        mapInitialized = true;
      };
    </script>
    <script src="[[*-bundle-*.js]]"></script>
    <script src='https://maps.googleapis.com/maps/api/js?key=<%- googleKey %>&signed_in=true&libraries=places&callback=initMap'
        async defer></script>
    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
    </script>

  </body>
</html>
